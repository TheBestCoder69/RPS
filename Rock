import random

# Declares the computer
computer = random.randint(1, 3)

# Sets up the Variables
rock = 1
si = 2
paper = 3

# Method for the computer to choose rock, paper, or si
def computer():
  match computer:
    case 1: 
      print rock
    case 2:
      print si
    case 3:
      print paper
        
      
